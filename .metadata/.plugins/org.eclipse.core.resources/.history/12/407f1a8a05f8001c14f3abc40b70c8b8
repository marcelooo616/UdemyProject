package exercicios;

import java.util.Scanner;

public class exercicioFor2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
    
		Scanner sc = new Scanner(System.in);
		
		int N = sc.nextInt();
		int x = 0;
		for(int i = 0; i < N; i++) {
			 x = sc.nextInt();
			
		}
		System.out.println(x);
	}
	

}
