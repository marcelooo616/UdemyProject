package exercicios;

import java.util.Scanner;

public class exercicioFor2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
    
		Scanner sc = new Scanner(System.in);
		
		int N = sc.nextInt();
		int numero = 0;
		for(int i = 0; i < N; i++) {
			int x = sc.nextInt();
			//numero = x;
			if (x >= 10 && x <= 20) {
				
				System.out.println(i);
				
			}
			
		}
		System.out.println(numero);
	}
	

}
