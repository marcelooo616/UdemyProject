package entiti;

public class Conta {

	private int number;
	private String nomeConta;
	private double saldo;
	
	public Conta(int number, String nome, double saldo) {
		this.nomeConta = nome;
		this.number = number;
		this.saldo = saldo;
	}
	public  void deposito(double valor) {
		this.saldo += valor;
		
		
	}
	
	public String toString() {
		return "Nome: "
				+nomeConta
				+"\nConta: "
				+number
				+"\nSaldo: "
				+ saldo;
	}
	
}
